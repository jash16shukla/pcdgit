#include<stdio.h>
int isprime(int p)
{
	int d;
	for(d=2;d<p/2;d++)
	{
		if(p%d==0)
			return 0;
		else
			return 1;
	}
}
int main()
{
	int n,flag=0;
	printf("Enter the number\n");
	scanf("%d",&n);
	flag=isprime(n);
	if(flag==1)
		printf("%d is prime number\n",n);
	else
		printf("%d is not a prime number\n",n);
	return 0;
}
